{
  "name": "gkd-monorepo",
  "private": true,
  "type": "module",
  "scripts": {
    "format": "prettier --cache --write --ignore-unknown .",
    "dev": "pnpm build:api && pnpm -F @gkd-kit/docs docs:dev",
    "build:api": "pnpm -F @gkd-kit/api predocs:build"
  },
  "dependencies": {
    "@gkd-kit/selector": "0.0.25",
    "@types/file-saver": "2.0.7",
    "@types/node": "20.11.30",
    "acorn-walk": "8.3.2",
    "cross-env": "7.0.3",
    "dom-serializer": "2.0.0",
    "file-saver": "2.0.5",
    "htmlparser2": "9.1.0",
    "magic-string": "0.30.10",
    "markdown-it-mathjax3": "4.3.2",
    "medium-zoom": "1.1.0",
    "prettier": "3.2.5",
    "tsx": "4.7.2",
    "typedoc": "0.25.13",
    "typedoc-plugin-markdown": "4.0.0-next.47",
    "typedoc-vitepress-theme": "1.0.0-next.10",
    "typescript": "5.4.5",
    "unocss": "0.59.4",
    "vitepress": "1.1.3"
  },
  "volta": {
    "node": "20.11.0"
  },
  "packageManager": "pnpm@9.4.0",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "search-insights",
        "vite"
      ]
    }
  }
}
